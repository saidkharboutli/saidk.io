{
  "name": "saidk-io",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "generate-types": "wrangler types .wrangler/worker-configuration.d.ts",
    "dev": "wrangler pages dev --proxy 4321 --kv PAGE_VIEWS -- npm run start",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro build && npm run postbuild && npx wrangler pages dev ./dist --kv PAGE_VIEWS",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "build-types": "tsc --noEmit --pretty",
    "prepare": "husky install",
    "postbuild": "pagefind --site dist --verbose",
    "deploy": "npm run build && npm run postbuild && npx wrangler pages deploy"
  },
  "devDependencies": {
    "@astrojs/react": "^4.4.2",
