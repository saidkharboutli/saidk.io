{
  "name": "astro-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler pages dev --proxy 4321 --kv PAGE_VIEWS -- npm run start",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro build && npm run postbuild && npx wrangler pages dev ./dist --kv PAGE_VIEWS",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.astro .",
    "build-types": "tsc --noEmit --pretty",
    "prepare": "husky install",
    "postbuild": "pagefind --site dist --verbose",
    "deploy": "npm run build && npm run postbuild && npx wrangler pages deploy dist"
  },
  "devDependencies": {
    "@astrojs/react": "^3.0.10",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/tailwind": "^5.1.0",
    "@cloudflare/workers-types": "^4.20240208.0",
    "@hbsnow/rehype-sectionize": "^1.0.7",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.55",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "astro-eslint-parser": "^0.16.3",
    "astro-imagetools": "^0.9.0",
    "astro-robots-txt": "^1.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-tailwindcss": "^3.14.2",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "pagefind": "^1.0.4",
    "path-browserify": "^1.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "wrangler": "^3.28.3"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^9.0.0",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@emailjs/browser": "^4.1.0",
    "@tsparticles/engine": "^3.1.0",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.1.0",
    "@types/jsonp": "^0.2.3",
    "astro": "^4.3.6",
    "astro-seo": "^0.8.0",
    "create-cloudflare": "^2.11.3",
    "jsonp": "^0.2.1",
    "npm-check-updates": "^16.14.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reaptcha": "^1.12.1",
    "sanitize-html": "^2.11.0",
    "tailwindcss-3d": "^1.0.5",
